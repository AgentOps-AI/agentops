# Dashboard (Next.js)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_PROJECT_ID=YOUR_PROJECT_ID

NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000

NEXT_PUBLIC_ENVIRONMENT_TYPE=development
NEXT_PUBLIC_PLAYGROUND=true

# Optional analytics/monitoring
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_SENTRY_ORG=
NEXT_PUBLIC_SENTRY_PROJECT=
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development

# Stripe (optional for local unless testing billing/webhooks)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
NEXT_STRIPE_SECRET_KEY=
NEXT_STRIPE_WEBHOOK_SECRET=

# API (FastAPI)
PROTOCOL=http
API_DOMAIN=localhost:8000
APP_DOMAIN=localhost:3000
ENVIRONMENT=development
SQLALCHEMY_LOG_LEVEL=WARNING

# Auth
JWT_SECRET_KEY=REPLACE_WITH_LONG_RANDOM_SECRET

# Supabase DB + Storage
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_MAX_POOL_SIZE=10
SUPABASE_HOST=YOUR_SUPABASE_PG_HOST
SUPABASE_PORT=5432
SUPABASE_DATABASE=postgres
SUPABASE_USER=postgres.YOUR_PROJECT_ID
SUPABASE_PASSWORD=YOUR_SUPABASE_DB_PASSWORD
SUPABASE_S3_BUCKET=user-uploads
SUPABASE_S3_LOGS_BUCKET=agentops-logs
SUPABASE_S3_ACCESS_KEY_ID=
SUPABASE_S3_SECRET_ACCESS_KEY=

# ClickHouse
# ClickHouse Configuration
# For LOCAL development, use:
#   CLICKHOUSE_HOST=127.0.0.1
#   CLICKHOUSE_PORT=8123
#   CLICKHOUSE_SECURE=false
#   CLICKHOUSE_PASSWORD=password
# For CLOUD deployment, use your ClickHouse Cloud credentials:
CLICKHOUSE_HOST=127.0.0.1  # Change to YOUR_CLICKHOUSE_HOST for cloud
CLICKHOUSE_PORT=8123        # Change to 8443 for cloud
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=password  # Set your password
CLICKHOUSE_DATABASE=otel_2
CLICKHOUSE_SECURE=false     # Change to true for cloud
CLICKHOUSE_ENDPOINT=
CLICKHOUSE_USERNAME=

# Stripe (server-side, optional)
STRIPE_SECRET_KEY=
STRIPE_SUBSCRIPTION_PRICE_ID=
STRIPE_TOKEN_PRICE_ID=
STRIPE_SPAN_PRICE_ID=
# STRIPE_WEBHOOK_SECRET=

# Cache (optional)
REDIS_HOST=localhost
REDIS_PORT=6379

# Demo org (optional)
DEMO_ORG_ID=c0000000-0000-0000-0000-000000000000
